{
    "name": "calculator-tn-api",
    "description": "calculator-tn-api",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "@middy/core": "^4.2.8",
        "@middy/do-not-wait-for-empty-event-loop": "^4.2.8",
        "@middy/http-error-handler": "^4.2.8",
        "@middy/http-event-normalizer": "^4.2.8",
        "@middy/http-json-body-parser": "^4.2.8",
        "bcrypt": "^5.1.0",
        "bcryptjs": "^2.4.3",
        "http-errors": "^2.0.0",
        "http-status-codes": "^2.2.0",
        "joi": "^17.9.1",
        "jsonwebtoken": "^9.0.0"
    },
    "devDependencies": {
        "aws-sdk-client-mock": "^2.0.0",
        "jest": "^29.2.1",
        "standard": "^17.0.0"
    },
    "scripts": {
        "dev": "sam local start-api --port 3000 --env-vars env.json --log-file calculator-api.log",
